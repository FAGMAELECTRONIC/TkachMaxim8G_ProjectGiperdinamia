@import url('https://fonts.googleapis.com/css2?family=Oswald&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Oswald:wght@700&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Overpass:ital,wght@1,200&display=swap');
h1{font-family: 'Oswald', sans-serif;}
h2{font-family: 'Oswald', sans-serif;}
h3{font-family: 'Oswald', sans-serif;}
h4{font-family: 'Oswald', sans-serif;}
h5{font-family: 'Oswald', sans-serif;}
a{font-family: 'Oswald', sans-serif;}
p{font-family: 'Overpass', sans-serif;}



*{
	margin: 0px;
	padding: 0px;
}
ul{
	display: flex;
	list-style-type: none;
}
a{
	padding-left: 10px;
	padding-right: 10px;
	color: white;
	text-decoration: none;
}
header{
	background-color: green;
	padding: 10px;
}

#Start{
	display: flex;
	align-items: center;
	flex-direction: column;
	background-image: URL(https://e-kolosok.org/wp-content/uploads/2020/07/hz35.png);
	background-size: cover;
	padding-bottom: 530px;
}
#Gtxt{
	font-size: 50px;
	text-shadow: 3px 3px 16px rgba(150, 150, 150, 1);
}
#s1{
	padding-top: 50px;
	margin: 20px;
	font-size: 25px;
	padding-bottom: 50px;
}
#s2{
	display: flex;
	justify-content: space-around;
}
#Pr{
	display: flex;
	align-items: center;
	flex-direction: column;
	-webkit-box-shadow: 54px 59px 100px -54px rgba(34, 60, 80, 0.41);
-moz-box-shadow: 54px 59px 100px -54px rgba(34, 60, 80, 0.41);
box-shadow: 54px 59px 100px -54px rgba(34, 60, 80, 0.41);
}